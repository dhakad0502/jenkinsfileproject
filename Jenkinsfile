pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('build') {
            steps {
                echo 'Hello World you are going to build '
            }
        }
        stage('Test') {
            steps {
                echo 'Hello World you are going to test '
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello World you are going to Deploy '
            }
        }
    }
}
